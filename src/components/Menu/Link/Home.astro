<style lang="scss">
  @import "/src/styles/mixins";

  .desktop {
    display: block;

    @include small-down {
      display: none;
    }
  }

  .mobile {
    display: block;

    @include small-up {
      display: none;
    }
  }

  .home-link {
    display: block;
  }

  a.home-link {
    @include focus {
      .focus {
        display: block;
        fill: white;
      }

      @include small-up {
        .logo {
          fill: $c-body-bg;
        }
        .focus {
          fill: black;
        }
      }
    }
  }
</style>
